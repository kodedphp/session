{
    "name": "koded/session",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Session library",
    "keywords": [
        "session",
        "php7"
    ],
    "authors": [
        {
            "name": "Mihail Binev",
            "homepage": "https://github.com/kodedphp/session"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.4",
        "psr/http-server-middleware": "^1.0",
        "koded/cache-simple": "~1",
        "koded/http": "*"
    },
    "autoload": {
        "psr-4": {
            "Koded\\Session\\": ""
        },
        "files": [
            "functions.php"
        ],
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Koded\\Session\\": "Tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7",
        "satooshi/php-coveralls": "~1",
        "ext-memcached": "*",
        "ext-redis": "*",
        "ext-json": "*"
    }
}
