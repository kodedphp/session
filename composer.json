{
    "name": "koded/session",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "A session library with custom handlers and php.ini support",
    "keywords": [
        "session",
        "session-handler",
        "memcached",
        "redis"
    ],
    "authors": [
        {
            "name": "Mihail Binev",
            "homepage": "https://github.com/kodedphp/session"
        }
    ],
    "require": {
        "php": "~7.3",
        "psr/http-server-middleware": "~1",
        "koded/cache-simple": "~2",
        "koded/http": "~1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Koded\\Session\\": ""
        },
        "files": [
            "functions.php"
        ],
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "suggest": {
        "ext-memcached": "*",
        "ext-redis": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Koded\\Session\\": "Tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7",
        "scrutinizer/ocular": "^1.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
