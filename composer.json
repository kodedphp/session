{
  "name": "koded/session",
  "type": "library",
  "license": "BSD-3-Clause",
  "description": "A session library with custom handlers and php.ini support",
  "keywords": [
    "session",
    "session-handler",
    "memcached",
    "redis"
  ],
  "authors": [
    {
      "name": "Mihail Binev",
      "homepage": "https://github.com/kodedphp/session"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^8",
    "psr/http-server-middleware": "^1",
    "koded/cache-simple": "3.*",
    "koded/http": "3.*",
    "ext-json": "*"
  },
  "autoload": {
    "classmap": [
      ""
    ],
    "files": [
      "functions.php"
    ],
    "exclude-from-classmap": [
      "Tests/",
      "build/"
    ]
  },
  "suggest": {
    "ext-memcached": "*",
    "ext-redis": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Koded\\Session\\": "Tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
